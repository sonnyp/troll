{
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "env": {
    "es2022": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "ARGV": "readonly",
    "Debugger": "readonly",
    "GIRepositoryGType": "readonly",
    "globalThis": "readonly",
    "imports": "readonly",
    "Intl": "readonly",
    "log": "readonly",
    "logError": "readonly",
    "print": "readonly",
    "printerr": "readonly",
    "window": "readonly",
    "TextEncoder": "readonly",
    "TextDecoder": "readonly",
    "console": "readonly",
    "setTimeout": "readonly",
    "setInterval": "readonly",
    "clearTimeout": "readonly",
    "clearInterval": "readonly"
  }
}
